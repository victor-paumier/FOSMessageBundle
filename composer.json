{
    "name": "vpaumier/message-bundle",
    "type": "symfony-bundle",
    "description": "Provides user-to-user messaging features for your Symfony application.",
    "keywords": ["symfony", "message", "messaging"],
    "homepage": "https://github.com/victor-paumier/FOSMessageBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Paumier",
            "email": "victor.paumier@gmail.com"
        }
    ],
    "repositories":[
        {
            "type": "vcs",
            "url":  "git@github.com:victor-paumier/FOSMessageBundle.git"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "doctrine/collections": "^1.3",
        "symfony/form": "^4.3|^5.0",
        "symfony/framework-bundle": "^4.3|^5.0",
        "symfony/twig-bundle": "^4.3|^5.0",
        "symfony/asset": "^4.3|^5.0",
        "twig/twig": "^2.0|^3.0"
    },
    "require-dev": {
        "symfony/validator": "^4.3|^5.0",
        "symfony/translation": "^4.3|^5.0",
        "symfony/yaml": "^4.3|^5.0",
        "symfony/security-bundle": "^4.3|^5.0",
        "symfony/templating": "^4.3|^5.0",
        "symfony/browser-kit": "^4.3|^5.0",
        "symfony/phpunit-bridge": "^4.2.8",
        "doctrine/orm": "^2.0"
    },
    "suggest": {
        "doctrine/doctrine-bundle": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "FOS\\MessageBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
